{
  "$schema": "https://json.schemastore.org/aspire-8.0.json",
  "resources": {
    "api": {
      "type": "project.v0",
      "path": "../FavoriteBusApp.Api/FavoriteBusApp.Api.csproj",
      "env": {
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EXCEPTION_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_EMIT_EVENT_LOG_ATTRIBUTES": "true",
        "OTEL_DOTNET_EXPERIMENTAL_OTLP_RETRY": "in_memory",
        "ASPNETCORE_FORWARDEDHEADERS_ENABLED": "true",
        "HTTP_PORTS": "{api.bindings.http.targetPort}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        },
        "https": {
          "scheme": "https",
          "protocol": "tcp",
          "transport": "http",
          "external": true
        }
      }
    },
    "reactvite": {
      "type": "container.v1",
      "build": {
        "context": "../favoritebusapp-client-reactvite",
        "dockerfile": "../favoritebusapp-client-reactvite/Dockerfile"
      },
      "env": {
        "NODE_ENV": "development",
        "services__api__http__0": "{api.bindings.http.url}",
        "services__api__https__0": "{api.bindings.https.url}",
        "BROWSER": "none",
        "VITE_PORT": "{reactvite.bindings.http.targetPort}"
      },
      "bindings": {
        "http": {
          "scheme": "http",
          "protocol": "tcp",
          "transport": "http",
          "port": 3000,
          "targetPort": 2999,
          "external": true
        }
      }
    }
  }
}